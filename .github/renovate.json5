{
  extends: ["config:base"],
  stabilityDays: 1,
  labels: [
    "dependencies",
  ],
  packageRules: [
    {
      datasources: ["maven"],
      labels: [
        "dependencies",
        "java",
      ],
    },
    {
      datasources: ["gradle-version"],
      labels: [
        "dependencies",
        "gradle",
      ],
    },
    {
      datasources: ["docker"],
      labels: [
        "dependencies",
        "docker",
      ],
    },
    {
      packageNames: ["com.google.guava:guava"],
      allowedVersions: "/^\\d+(\\.\\d+)+-jre$/",
    },
    {
      packageNames: ["net.sourceforge.plantuml:plantuml"],
      allowedVersions: "/^\\d+\\.\\d+(\\.\\d+)+$/",
    },
  ],
  "git-submodules": {
    enabled: false,
    labels: [
      "dependencies",
      "submodules",
    ],
  },
}
